#include <stdio.h>
#include <stdlib.h>
#pragma warning(disable:4996);

using namespace std;

/*
【題號】
	11063, B2-Sequence

【題目大意】
	所謂「B2數列」係指一數列 1 <= b1 < b2 < b3 ...
	其中所有的 bi + bj （i <= j）皆不相等
	試判別某一數列是否為「B2數列」

【解題思維】
	輸入時確保測資按照漸大排列
	兩兩捉對比較，儲存到combi[]陣列中
	確認沒有重複的值
*/

int main() {

	int lenghth;
	int squence[100];
	int IsB2;
	int test = 1;

	while (scanf("%d", &lenghth) != EOF) {

		int combi[20000] = { 0 };
		IsB2 = 1;

		for (int i = 0; i < lenghth; i++) {
			scanf("%d", &squence[i]);
			if (i > 0 && squence[i] < squence[i - 1])
				IsB2 = 0;
		}

		if (IsB2) {
			for (int i = 0; i < lenghth; i++) {
				for (int j = i; j < lenghth; j++) {
					if (combi[squence[i] + squence[j]] == 1) {
						IsB2 = 0;
						break;
					}
					combi[squence[i] + squence[j]] = 1;
				}
				if (IsB2 == 0) break;
			}
		}
		if (IsB2)
			printf("Case #%d: It is a B2-Sequence.\n\n", test);
		else
			printf("Case #%d: It is not a B2-Sequence.\n\n", test);

		test++;
	}
	return 0;
}
